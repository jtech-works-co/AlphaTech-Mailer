{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"public": true,
		"**/node_modules": true,
		"**/src/index.css": true,
		"**/src/react-app-env.d.ts": true
	},
	"liveSassCompile.settings.formats": [
		{
			"format": "expanded",
			"extensionName": ".css",
			"savePath": "/src",
			"savePathReplacementPairs": null
		}
	],
	"liveSassCompile.settings.excludeList": [
		"/**/node_modules/**",
		"/.vscode/**",
		"/alphatech-mailer/**",
		"/api/**"
	],
	"liveSassCompile.settings.generateMap": false,
	"liveSassCompile.settings.showAnnouncements":false,
	"livePreview.showServerStatusNotifications": false,
	"liveSassCompile.settings.showOutputWindowOn": "None"
}